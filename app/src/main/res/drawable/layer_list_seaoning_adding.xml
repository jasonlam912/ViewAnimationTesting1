<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="left|bottom"
        android:height="30dp"
        android:width="30dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="#9CCC65"/>
            <stroke android:width="2dp"
                android:color="#66BB6A"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_spices"
        android:height="22dp"
        android:width="22dp"
        android:gravity="center"
        android:right="5dp"
        android:top="5dp"/>
    <item android:gravity="top|right">
        <layer-list>
            <item android:width="15dp"
                android:height="5dp"
                android:gravity="center">
                <shape android:shape="rectangle">
                    <solid android:color="#C5E1A5"/>
                </shape>
            </item>
            <item android:width="5dp"
                android:height="15dp"
                android:gravity="center">
                <shape android:shape="rectangle">
                    <solid android:color="#C5E1A5"/>
                </shape>
            </item>
            <item android:width="13dp"
                android:height="3dp"
                android:gravity="center">
                <shape android:shape="rectangle">
                    <solid android:color="#DCEDC8"/>
                </shape>
            </item>
            <item android:width="3dp"
                android:height="13dp"
                android:gravity="center">
                <shape android:shape="rectangle">
                    <solid android:color="#DCEDC8"/>
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>